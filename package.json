{
  "name": "bp",
  "version": "0.0.4",
  "description": "nodejs package using typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "npx jest",
    "cov": "npx jest --coverage",
    "prebuild": "npm run clean",
    "build": "npx tsc",
    "dev": "npx tsc --watch",
    "clean": "npx rimraf ./lib",
    "format": "npx prettier-tslint fix ./src/**/*.ts",
    "preversion": "npm run build && npm t",
    "postversion": "npm run push",
    "lint": "npx tslint -p tsconfig.json",
    "prepush": "npm run format",
    "push": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "author": "GHIAT Houssem",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.4",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "prettier-tslint": "^0.4.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "node-simplecqrs": "^0.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houssemdevs/bpnodepkg.git"
  },
  "bugs": {
    "url": "https://github.com/houssemdevs/bpnodepkg/issues"
  },
  "homepage": "https://github.com/houssemdevs/bpnodepkg#readme"
}
